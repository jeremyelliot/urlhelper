{
    "name": "jeremyelliot/urlhelper",
    "description": "Helper class for parsing URLs",
    "keywords": ["parse", "url"],
    "license": "MIT",
    "type": "library",
    "authors": [
    	{
    		"name": "Jeremy Elliot",
    		"email": "jeremy@openweb.co.nz",
    		"role": "Developer"
    	}
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "UrlHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
